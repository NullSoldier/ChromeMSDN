{
  "manifest_version": 2,

  "name": "Hide MSDN Inherited Members",
  "description": "When visiting an MSDN page, this extension hides inherited members",
  "version": "1.0",
	"permissions": ["tabs", "http://*/*" ],
  "browser_action": { "default_icon": "icon48.png" },
	"background": { "scripts": ["background.js"] },
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}
